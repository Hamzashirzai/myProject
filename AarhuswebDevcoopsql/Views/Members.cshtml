@using System.Collections
@using Umbraco.Core;
@using Umbraco.Core.Models;
@using Umbraco.Core.Services;
@using Umbraco.Core.Persistence
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
}
<div class="loginpage">
	@Umbraco.Field("pagetextcontent")
	<div class="row">
		<div class="col-md-6"><h1>Active</h1>
			@Html.Partial("prospectmembers");
			
		</div>

		<div class="col-md-6"><h1>Prospect</h1>
		
		
		@{
			var memberService =
			ApplicationContext.Current.Services.MemberService;

			var members = memberService.GetMembersByGroup("prospect");
			}
			@foreach (var member in members) 
			{
				
			<h3>
			@(member.GetValue("firstname") + " "
			+ member.GetValue("lastname")) <br/>
				@member.GetValue("mobile")<br />
			
			</h3>
				
			}
		


		</div>
	

	
	</div>
</div>